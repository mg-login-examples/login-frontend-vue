{
  "name": "frontend-login",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build-prod": "run-p type-check \"build-only-prod {@}\" --",
    "build-dev": "run-p type-check \"build-only-dev {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:unit:run": "vitest run --environment jsdom --root src/",
    "test:unit:coverage": "vitest run --environment jsdom --root src/ --coverage",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "test:e2e:dev:run": "start-server-and-test 'vite dev --port 4173' :4173 'cypress run --e2e --headed'",
    "test:e2e:dev:run:docker": "cypress run --e2e",
    "test:e2e:dev:docker": "cypress open --e2e",
    "test:e2e:playwright": "playwright test",
    "build-only-prod": "vite build",
    "build-only-dev": "vite build --mode development",
    "build-only-dev-watch": "vite build --mode development --watch",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "mitt": "^3.0.1",
    "password-validator": "^5.3.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue3-google-login": "^2.0.26"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.3",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@pinia/testing": "^0.1.3",
    "@playwright/test": "^1.42.1",
    "@rushstack/eslint-patch": "^1.10.1",
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "allure-js-commons": "^2.15.0",
    "allure-playwright": "^2.15.0",
    "autoprefixer": "^10.4.19",
    "casual": "^1.6.2",
    "cypress": "^13.7.2",
    "cypress-mailslurp": "^1.9.0",
    "docker-secret": "^2.0.6",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-playwright": "^1.5.2",
    "eslint-plugin-vue": "^9.24.0",
    "jsdom": "^24.0.0",
    "mock-websocket": "^0.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.3",
    "vite": "^5.2.8",
    "vite-plugin-vue-devtools": "^7.0.18",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.7"
  }
}
