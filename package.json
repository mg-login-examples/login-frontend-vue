{
  "name": "frontend-login",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build:prod": "run-p type-check build-only",
    "build:dev": "run-p type-check build-only:dev",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:unit:run": "vitest run --environment jsdom --root src/",
    "test:unit:coverage": "vitest run --environment jsdom --root src/ --coverage",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "test:e2e:dev:run": "start-server-and-test 'vite dev --port 4173' :4173 'cypress run --e2e --headed'",
    "test:e2e:dev:run:docker": "cypress run --e2e",
    "test:e2e:dev:docker": "cypress open --e2e",
    "test:e2e:playwright": "playwright test",
    "build-only": "vite build",
    "build-only:dev": "vite build --mode development",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "mitt": "^3.0.0",
    "password-validator": "^5.3.0",
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^13.1.0",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@pinia/testing": "^0.0.14",
    "@playwright/test": "^1.29.2",
    "@rushstack/eslint-patch": "^1.1.4",
    "@shelex/cypress-allure-plugin": "^2.34.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitest/coverage-c8": "^0.24.5",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "allure-js-commons": "^2.0.0-beta.20",
    "autoprefixer": "^10.4.13",
    "cypress": "^10.11.0",
    "cypress-mailslurp": "^1.5.0",
    "docker-secret": "^2.0.5",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.1",
    "minifaker": "^1.34.1",
    "mock-websocket": "^0.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.2.2",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.8"
  }
}
